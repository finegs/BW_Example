                <config>
                    <variableName>tempStr</variableName>
                </config>
                <pd:inputBindings>
                    <root>
                        <tempStr>
                            <xsl:value-of select="concat( if(string-length($tempStr/root/tempStr) > 0) then concat($tempStr/root/tempStr, &quot;; &quot;) else '', $concatTests/root/testStr)"/>
                        </tempStr>
                    </root>
                </pd:inputBindings>
            </pd:activity>
