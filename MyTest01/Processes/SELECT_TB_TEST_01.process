                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </element>
                </config>
                <pd:inputBindings>
                    <root>
                        <testStr>
                            <xsl:value-of select="concat(&quot;a=&quot;, $curRec/Record/A, &quot;, &quot;, &quot;name=&quot;, $curRec/Record/NAME)"/>
                        </testStr>
                    </root>
                </pd:inputBindings>
            </pd:activity>
            <pd:activity name="Assign">
                <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
                <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
                <pd:x>861</pd:x>
